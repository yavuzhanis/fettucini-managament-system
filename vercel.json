{
    "builds": [
      {
        "src": "restaurant_management/wsgi.py",
        "use": "@vercel/python",
        "config":{"maxLambdaSize":"25mb","runtime":"python3.9"}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "restaurant_management/wsgi.py"
      }
    ]
  }
  
