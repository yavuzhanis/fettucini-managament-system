{
  "version": 2,
  "builds": [
    {
      "src": "restaurant_management/manage.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "restaurant_management/manage.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "restaurant_management.settings"
  },
  "build": {
    "env": {
      "VERCEL": "1"
    },
    "commands": {
      "post-build": "python restaurant_management/manage.py migrate"
    }
  }
}
